# Image Processing Configuration
# Enable smart compression to reduce API costs and processing time
COMPRESSION_ENABLED=true
# JPEG quality (1-100) for compressed images. Higher = better quality but larger size
COMPRESSION_QUALITY=85
# Maximum dimension in pixels. Images larger than this will be scaled down
MAX_DIMENSION=1024

# Batch Processing
# Number of images to process in parallel. Adjust based on your API limits and system resources
DEFAULT_BATCH_SIZE=8
# Maximum allowed batch size. Some models have specific limits (e.g., Gemini Flash = 16)
MAX_BATCH_SIZE=16

# Output Configuration
# Format for saving results: 'csv' for tabular data, 'xml' for structured data
DEFAULT_OUTPUT_FORMAT=csv
# Directory where output files will be saved. Will be created if it doesn't exist
OUTPUT_DIRECTORY=output

# Model Configuration
# AI model to use for image analysis. Available options:
# - gemini-2.0-flash-exp: Latest experimental model, fastest
# - gemini-1.5-flash: Production model, good balance
# - gemini-1.5-pro: More detailed analysis but slower
DEFAULT_MODEL=gemini-2.0-flash-exp
# Enable automatic renaming of processed files with metadata-based names
RENAME_FILES=false
# Prefix to add to renamed files (e.g., "processed_" or "analyzed_")
FILE_PREFIX=

# Metadata Configuration
# Create backup copies of files before modifying their metadata
BACKUP_METADATA=true
# Write analysis results directly to image EXIF data for better integration
WRITE_EXIF=true
# Create duplicate files before modifying (original files remain untouched)
DUPLICATE_FILES=false
# Suffix to add to duplicate files (e.g., "_modified")
DUPLICATE_SUFFIX=_modified

# Progress and Logging
# Show real-time progress bars and processing statistics
SHOW_PROGRESS=true
# Show real-time Gemini model responses
VERBOSE_OUTPUT=false
# Logging level: DEBUG, INFO, WARNING, ERROR, or CRITICAL
LOG_LEVEL=INFO

# API Keys
# Your Google API key with Gemini Vision API access enabled
# Get it from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your-google-api-key-here

# Paths
# Path to ExifTool executable. Default location is in resources directory
EXIFTOOL_PATH=resources/exiftool/exiftool

# Advanced Settings (uncomment to use)
# Maximum retries for failed API calls
# MAX_RETRIES=3

# Timeout in seconds for API calls
# API_TIMEOUT=30

# Enable debug mode for more detailed logging
# DEBUG_MODE=false

# Custom model parameters
# MODEL_TEMPERATURE=0.7
# MAX_TOKENS=1000

# Cache settings
# ENABLE_CACHE=true
# CACHE_DIR=.cache
# CACHE_TTL=3600  # Time-to-live in seconds 